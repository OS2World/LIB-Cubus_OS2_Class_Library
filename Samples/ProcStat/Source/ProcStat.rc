/* Procstat.rc */
#include <os2.h>
#include "..\\Source\\Procstat.h"


ICON PROCSTAT_MAIN "..\\Source\\PROCESS.ICO"

MENU PROCSTAT_MAIN
BEGIN 
  SUBMENU "File", PROCSTAT_MENU_FILE, MIS_TEXT
  BEGIN
    MENUITEM "Set Font", PROCSTAT_SETFONT, MIS_TEXT
    MENUITEM "Refresh", PROCSTAT_REFRESH, MIS_TEXT
    MENUITEM SEPARATOR
    MENUITEM "Quit", PROCSTAT_QUIT, MIS_TEXT
  END
END

ACCELTABLE PROCSTAT_MAIN
BEGIN
  VK_F3, PROCSTAT_QUIT, VIRTUALKEY
END


/* end of source */
