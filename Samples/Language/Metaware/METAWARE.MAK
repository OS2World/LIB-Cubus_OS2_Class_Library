!include $(OCLSAMP)\COMMON.INC
!include ..\LISTBOX.INC

MAKEDEP = METAWARE.MAK
CFLAGS = $(S_HIGHCPP_COMPILE_DEBUG)
LFLAGS = $(S_HIGHCPP_LINK_DEBUG)

{..\Source}.cpp.obj:
   hc $(CFLAGS) %s

